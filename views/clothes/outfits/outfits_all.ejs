<% include ../../partials/boilerplate %>
<% console.log(data) %>
    <h1> <%= username %> outfits </h1>

    <% data.map((outfit)=>{ %>
        <p><%= outfit.name %> </p>
        <img src="<%= outfit.imagebest %>" />
        <p> <%= outfit.brand %> </p>
        <p> <%= outfit.description %> </p>
        <p> <%= outfit.price %> </p>
        <p> <%= outfit.productid %> </p>
        <p> <%= outfit.brandename %> </p>
        <p> <%= outfit.clothing_type %> </p>
    <% }) %>

<% include ../../partials/end %>